<%- include("../partials/header")%>

<!-- <h1>Welcome to {USERNAME}'s PC.</h1> -->


<img class ="img-teams" src ="https://64.media.tumblr.com/641ec476865c68bb4f6f7bf6928c1d5c/tumblr_pevr8icX581vw1ceco1_1280.pnj">

<% teams.forEach(t => { %>
    <div>
        <p> <%=t.name%></p>
        <%t.pokemon.forEach(p => { %>
            <img src="<%=p.sprite%>">
        <% }) %>

    </div>
    <a href="/teams/<%=t._id%>">Team Details</a>
<% }) %>


<button><a href="/teams/new">Create New Team</a></button>

